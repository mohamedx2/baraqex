{
  "name": "baraqex",
  "version": "1.0.44",
  "description": "A powerful full-stack framework built on top of frontend-hamroun",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./wasm": {
      "types": "./dist/wasm.d.ts",
      "import": "./dist/wasm.js"
    }
  },
  "browser": {
    "./dist/index.js": "./dist/browser.js",
    "crypto": false,
    "fs": false,
    "path": false,
    "http": false,
    "https": false,
    "express": false,
    "cors": false,
    "mongodb": false,
    "mysql2": false,
    "pg": false,
    "bcryptjs": false,
    "jsonwebtoken": false
  },
  "files": [
    "dist/",
    "templates/",
    "bin/",
    "README.md"
  ],
  "bin": {
    "baraqex": "./bin/baraqex.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build:esm": "node build.mjs",
    "clean": "rimraf dist",
    "test": "jest",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "framework",
    "frontend",
    "backend",
    "fullstack",
    "react",
    "ssr",
    "wasm",
    "go"
  ],
  "author": "Mohamed X",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^4.0.3",
    "frontend-hamroun": "^1.2.35",
    "open": "^10.1.2",
    "ws": "^8.18.2"
  },
  "peerDependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "cors": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "bcryptjs": {
      "optional": true
    },
    "jsonwebtoken": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.4",
    "esbuild": "^0.19.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^6.0.0",
    "mysql2": "^3.6.0",
    "pg": "^8.11.0"
  }
}
