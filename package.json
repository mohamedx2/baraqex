{
  "name": "baraqex",
  "version": "1.0.51",
  "description": "A powerful server-side framework built on Frontend Hamroun",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./wasm": {
      "types": "./dist/wasm.d.ts",
      "import": "./dist/wasm.js"
    }
  },
  "files": [
    "dist/",
    "templates/",
    "bin/",
    "README.md",
    "README_WASM.md"
  ],
  "bin": {
    "baraqex": "./bin/baraqex.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:js": "node build.js",
    "clean": "rimraf dist",
    "dev": "npm run build && node dev-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "frontend-hamroun": "^1.2.35"
  },
  "peerDependencies": {
    "express": ">=4.18.0",
    "cors": ">=2.8.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "cors": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "bcryptjs": {
      "optional": true
    },
    "jsonwebtoken": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "frontend-hamroun",
    "server",
    "framework",
    "ssr",
    "fullstack",
    "webassembly",
    "wasm",
    "api",
    "database",
    "auth"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/baraqex.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/baraqex/issues"
  },
  "homepage": "https://github.com/yourusername/baraqex#readme"
}
