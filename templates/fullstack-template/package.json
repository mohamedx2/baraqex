{
  "name": "fullstack-baraqex-app",
  "version": "0.1.0",
  "private": true,
  "description": "A fullstack application built with Baraqex",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "esbuild src/client.tsx --bundle --outfile=dist/client.js --format=esm --target=es2020 --platform=browser --external:express --external:path --external:fs --external:http",
    "build:server": "esbuild src/server.ts --bundle --outfile=dist/server.js --platform=node --target=node16 --format=esm --external:express --external:cors --external:jsonwebtoken --external:bcryptjs --external:mongodb --external:mysql2 --external:pg --sourcemap",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js"
  },
  "dependencies": {
    "baraqex": "^1.0.60",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "frontend-hamroun": "latest",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "latest"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "mongodb": "^5.7.0",
    "mysql2": "^3.6.0",
    "pg": "^8.11.3"
  },
  "keywords": [
    "baraqex",
    "fullstack",
    "react",
    "typescript",
    "express",
    "api",
    "authentication"
  ],
  "author": "",
  "license": "MIT"
}
